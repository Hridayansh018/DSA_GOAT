    "class Solution:\n",
    "    def getSum(self, a: int, b: int) -> int:\n",
    "        self.a = a\n",
    "        self.b = b\n",
    "\n",
    "        while b != 0:\n",
    "            carry = a & b\n",
    "\n",
    "            a = a^b\n",
    "            b = carry << 1\n",
    "        return a"
   ]
  }
